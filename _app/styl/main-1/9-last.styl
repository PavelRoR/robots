#last
	padding 100px 0
	background-color: #313B5A
	background-image: url(../img/backs/last.jpg)
	background-repeat: no-repeat
	background-position: center
	background-size: cover
	text-align center
	color: #fff
.last-title
	font-family: GM
	font-size: 35px
	font-weight: 900
	text-transform uppercase
	width 100%
	max-width 850px
	margin 0 auto
.footer-title
	font-family: GM
	font-size: 20px
	font-weight: 900
	text-transform uppercase
	margin 50px auto 30px
	width 100%
	max-width 750px
.soc-link
	width 50px
	height: 50px
	border-radius: 50px
	text-align center
	margin 0 5px
	display inline-block
	background-image: url(../img/icons/soc-links.png)
	background-repeat: no-repeat
	text-decoration none
	transition: all .3s ease
	margin-bottom: 50px
	&:focus
		text-decoration none
	&:hover
		transform: translateY(-4px)
	&-vk
		background-position: 0 0
		box-shadow: 0px 1px 25.23px 3.77px rgba(77, 118, 161, 0.79);
		background-color #4d76a1
	&-yt
		background-position: -50px 0
		box-shadow: 0px 1px 25.23px 3.77px rgba(255, 0, 0, 0.79);
		background-color #ff0000
	&-insta
		background-position: -100px 0
		box-shadow: 0px 1px 25.23px 3.77px rgba(240, 0, 117, 0.79);
		background-color #f00075
	&-tg
		background-position: -150px 0
		box-shadow: 0px 1px 25.23px 3.77px rgba(3, 155, 229, 0.79);
		background-color #039be5
	&-fb
		background-position: -200px 0
		box-shadow: 0px 1px 25.23px 3.77px rgba(59, 89, 152, 0.79);
		background-color #3b5998