#club
	padding 90px 0 50px
	background-color: #152333
	background-image: url(../img/backs-2/club.jpg)
	background-repeat no-repeat
	background-position: center
	background-size: cover
	@media screen and (max-width 560px)
		padding 30px 0
.general-title-club
	display inline-block
	&:before
		content: ''
		display block
		position absolute
		background-color: #cc922f
		width 350px
		height 78px
		top -30px
		right -30px
		@media screen and (max-width 991px)
			left 0
			right 0
			margin 0 auto
.club-subtitle
	margin-bottom: 30px
	color: #fff
	width 100%
	max-width 890px
.club-wrpper-list
	li
		list-style none
		padding-left: 26px
		margin-bottom: 30px
		position relative
		font-family: GM
		&:before
			content: ''
			display block
			position absolute
			top 5px
			left 0
			border-radius: 50px
			background-color: #CC922F
			width 14px
			height: 14px
		@media screen and (max-width 560px)
			margin-bottom: 20px
		@media screen and (max-width 385px)
			margin-bottom: 10px
.general-img-club
	img
		right 0
		top 70px
		z-index 0
	@media screen and (max-width 1199px)
		img
			right auto
			left -200px